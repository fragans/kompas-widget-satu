---
// imports
import WidgetCoronaReport from '../../components/widget/CoronaReport.vue'
import LayoutDefault from '../../layouts/Default.astro';
const pageTitle = 'Widget Corona Report'
// datas
const response = await fetch(`${import.meta.env.SECRET_WWW_HOST}/api/widget/covid`);
const data = await response.json();
const { covid } = data

---
<LayoutDefault title={pageTitle}>
  <WidgetCoronaReport data={covid} />
</LayoutDefault>
